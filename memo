$ docker build -t flowstar -f ./Dockerfile .

# hybrid system のサンプルを実行
$ docker run --rm -it -v "${PWD}/models:/models/" flowstar bash -c "cd /models; cat /models/hybrid_example1.model | /flowstar/flowstar"

# 論文の Figure2 
$ docker run --rm -it -v "${PWD}/models:/models/" flowstar bash -c "cd /models; cat /models/figure2_dnn_to_hybrid.model | /flowstar/flowstar"

